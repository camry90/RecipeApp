<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="StyleTitle14" parent="TextAppearance.AppCompat" >
        <item name="android:textSize">14sp</item>
        <item name="android:fontFamily">@font/montserrat_alternates_semibold</item>
        <item name="textAllCaps">true</item>
        <item name="android:textColor">#FFFFFF</item>
    </style>
    
    <style name="StyleNavButton" parent="Widget.Material3.Button" >
        <item name="android:textAppearance">@style/StyleTitle14</item>
        <item name="shapeAppearanceOverlay">@style/shapeAppearanceOverlay.NavButton</item>
        <item name="android:insetTop">0dp</item>
        <item name="android:insetBottom">0dp</item>
        <item name="android:insetLeft">0dp</item>
        <item name="android:insetRight">0dp</item>
    </style>
    
    <style name="shapeAppearanceOverlay.NavButton">
        <item name="cornerRadius">8dp</item>
    </style>


</resources>